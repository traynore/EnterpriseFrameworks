
@model IEnumerable<GWSApp.Models.ImportCustomer>


@{
    ViewBag.Title = "ImportCsv";
}

<h2>Import CSV File</h2>

<p>Warning - Importing CSV file will erase Database!</p>

<form action="" method="post" enctype="multipart/form-data">
    <table>
        <tr>
            <td><label for="file">Import CSV File:</label></td>
            <td><input type="file" name="file" id="file" /></td>
            <td><input type="submit" value="Import" /></td>
        </tr>
    </table>
</form>
</br>
@if (Model != null)
{
    <p>CSV File @ViewBag.FileName Imported Successfully and Saved to Database!</p>
}
</br>
@Html.ActionLink("Back To Index", "Index")

